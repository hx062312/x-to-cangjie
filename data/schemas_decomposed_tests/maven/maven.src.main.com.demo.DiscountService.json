                        "    public String getDiscountDescription() {\n",
                        "        switch (discountType) {\n",
                        "            case NONE:\n",
                        "                return \"No Discount\";\n",
                        "            case VIP:\n",
                        "                return \"VIP Discount (15% off)\";\n",
                        "            case FIXED_AMOUNT:\n",
                        "                return String.format(\"Fixed Discount ($%.2f off)\", discountValue);\n",
                        "            case PERCENTAGE:\n",
                        "                return String.format(\"Discount (%.0f%% off)\", discountValue);\n",
                        "            default:\n",
                        "                return \"No Discount\";\n",
                        "        }\n",
