                "37-54:calculateDiscount": {
                    "start": 37,
                    "end": 54,
                    "body": [
                        "    public double calculateDiscount(double subtotal) {\n",
                        "        if (subtotal <= 0) {\n",
                        "            return 0;\n",
                        "        }\n",
                        "\n",
                        "        switch (discountType) {\n",
                        "            case NONE:\n",
                        "                return 0;\n",
                        "            case VIP:\n",
                        "                return subtotal * 0.15;  // 15% VIP discount\n",
                        "            case FIXED_AMOUNT:\n",
                        "                return Math.min(discountValue, subtotal);  // Cap at subtotal\n",
                        "            case PERCENTAGE:\n",
                        "                return subtotal * (discountValue / 100.0);\n",
                        "            default:\n",
                        "                return 0;\n",
                        "        }\n",
                        "    }\n"
                    ],
